{
  "name": "styled-mdl",
  "version": "0.3.2",
  "description": "Components for React built using Styled Components and based on Google's Material Design Lite",
  "private": false,
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/isogon/styled-mdl.git"
  },
  "author": "David Weedon",
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn test:clean && rimraf ./lib",
    "build": "babel src -s -d lib --ignore '/__demos__/,/__tests__/'",
    "dev:serveStories": "NODE_ENV=development yarn storybook",
    "dev:createStories": "./scripts/demos2stories.js",
    "dev": "concurrently -k \"yarn dev:serveStories\" \"yarn dev:createStories\"",
    "format": "prettier --config ./.prettierrc --write \"{src,tests,stories}/**/*.js\"",
    "format:check": "prettier --config ./.prettierrc --list-different \"{src,tests,stories}/**/*.js\"",
    "lint": "eslint src testSetup scripts stories",
    "lint:staged": "lint-staged",
    "oncommit": "yarn format:check && yarn lint:staged",
    "pretest": "yarn test:clean && yarn lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "manualPrepublish": "yarn build && prepare-isogon-package . && yarn generate-lock-entry --production > yarn.lock",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "testSetup/**/*.js": "eslint",
    "scripts/**/*.js": "eslint",
    "stories/**/*.js": "eslint"
  },
  "pre-commit": "oncommit",
  "peerDependencies": {
    "react": "^15.4.1 || ^16.2.0",
    "react-dom": "^15.4.1 || ^16.2.0",
    "styled-components": "^2.0.0 || ^3.0.0"
  },
  "dependencies": {
    "@storybook/cli": "^3.4.2",
    "fontfaceobserver": "2.0.7",
    "invariant": "^2.2.4",
    "lodash": "4.17.2",
    "prop-types": "^15.6.0",
    "react-modal": "^3.1.0",
    "react-portal": "^4.1.0",
    "recompose": "^0.26.0",
    "styled-tools": "^0.2.2",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "@isogon/prepare-package": "^0.0.3",
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-plugin-dynamic-import-node": "1.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "6.16.0",
    "concurrently": "^3.5.1",
    "cross-env": "^3.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "glob": "^7.1.2",
    "jest": "^22.4.2",
    "jest-enzyme": "^4.2.0",
    "lint-staged": "^7.0.0",
    "pre-commit": "1.1.3",
    "prettier": "^1.11.1",
    "react": "^16.2.0",
    "react-children-with": "^1.1.4",
    "react-dom": "^16.2.0",
    "rimraf": "2.5.4",
    "styled-components": "^3.0.0"
  }
}
